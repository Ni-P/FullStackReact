{
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
            "jsx": true
        }
  },
  "env": {
        "node": true,
        "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
        "no-unused-vars": ["warn", { "vars": "all", "args": "after-used", "ignoreRestSiblings": false }],
        "no-console": ["warn"]
    }
}
